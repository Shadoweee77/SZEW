<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:views="SZEW_MAUI.Views.AdminShell"
       x:Class="SZEW_MAUI.Views.AdminShell"
       Title="SZEW - Administrator">
    <TabBar>
        <Tab Title="Strona główna">
            <ShellContent ContentTemplate="{DataTemplate views:AdminMainPage}" />
        </Tab>
        <Tab Title="Użytkownicy">
            <ShellContent ContentTemplate="{DataTemplate views:UsersPage}" />
        </Tab>
        <Tab Title="Zapotrzebowanie na narzędzia">
            <ShellContent ContentTemplate="{DataTemplate views:ToolRequestsPage}" />
        </Tab>
        <Tab Title="Generuj zestawienie">
            <ShellContent ContentTemplate="{DataTemplate views:SummaryPage}" />
        </Tab>
        <Tab Title="Moje ustawienia">
            <ShellContent ContentTemplate="{DataTemplate views:SettingsPage}" />
        </Tab>
    </TabBar>
</Shell>